<% if movement.valid? %>
document.getElementById("ui-modal-placeholder").innerHTML = ""; // remove modal
document.getElementById("movements-list-component-placeholder").innerHTML = "<%= j render(Movement::ListComponent.new(movements: movements, show_depot: true)) %>";
<% else %>
document.getElementById("ui-modal-placeholder").innerHTML = "<%= j render(Movement::ModalFormComponent.new(movement, depot_id, current_user)) %>";
<% end %>
