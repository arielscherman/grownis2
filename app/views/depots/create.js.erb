<% if @depot.valid? %>
document.getElementById("ui-modal-placeholder").innerHTML = ""; // remove modal
placeholder = document.getElementById("depots-list-placeholder")
placeholder.innerHTML = placeholder.innerHTML + "<%= j render Depot::CardComponent.new(depot: @depot) %>";
<% else %>
document.getElementById("ui-modal-placeholder").innerHTML = "<%= j render('depots/modal_form') %>";
<% end %>
